{
  "version": 2,
  "builds": [
    {
      "src": "house_rent/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "house_rent/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "house_rent.settings",
    "PYTHONPATH": "$ROOT"
  }
}
